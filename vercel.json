{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "builds": [],
  "functions": {
    "api/trading.py": {
      "runtime": "python3.12"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/trading.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/trading.py"
    }
  ]
}

