{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "builds": [
    {
      "src": "/api/trading.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/trading.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/trading.py"
    }
  ]
}

